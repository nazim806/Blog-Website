<%- include('partials/header'); -%>

<h1>Compose</h1>

<form class="" , action="/compose" , method="post">

    <div class="form-group">

        <label>Title</label>

        <input type="text" , class="form-control" name="postName">

        <label>Post</label>

        <textarea name="postContent" class="form-control" cols="40" rows="5"></textarea>

    </div>

    <button type="submit" class="btn btn-primary" name="button">Publish</button>

</form>



